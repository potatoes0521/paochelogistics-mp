@import '../../assets/css/theme.styl'

.call-service-wrapper
  height 64px
  padding-left 20px
  padding-right 12px
  background-color rgba(255,255,255,0.82)
  border-radius 200px 0px 0px 200px
  position fixed
  right 0
  top 40px
  display flex
  align-items center
  justify-content center
  z-index 2
.call-service-text
  font-size 30px
  color $font-color
  margin-left 10px

.call-service-icon
  font-size 48px
  color $theme-color