@import "../../assets/css/theme"

.about-wrapper
  width 100%
  border-top 2px solid #f5f5f5
  border-bottom 2px solid #f5f5f5
  padding-left 48px
  box-sizing border-box
.list-item
  width 100%
  height 108px
  display flex
  align-items center
  box-sizing border-box
  padding-right 48px
  justify-content space-between
  border-bottom 2px solid #f5f5f5
  color $font-color
  font-size 30rpx
.list-item:last-child
  border none

