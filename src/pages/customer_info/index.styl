@import '../../assets/css/theme.styl' 

page 
  width 100%
  height 100%

.page-wrapper
  width 100%
  height 100%
  display flex
  flex-direction column
  justify-content space-between

.search-wrapper
  width 100%
  height 108px
  box-sizing border-box
  padding 16px 24px
  border-top 2px solid #f5f5f5

.search-main
  width 100%
  height 100%
  background-color #f5f5f5
  border-radius 8px
  box-sizing border-box
  padding 24px
  display flex
  align-items center
  
.icon-search-style
  width 32px
  height 32px
  font-size 32px
  color $font-h-color


