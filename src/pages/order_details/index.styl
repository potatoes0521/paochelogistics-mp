@import "../../assets/css/theme.styl"
page 
  width 100%
  height 100%

.page-wrapper
  width 100%
  height 100%
  display flex
  flex-direction column
  justify-content space-between
  border-top 2px solid #F5F5F5
  box-sizing border-box

.page-main
  width 100%
  overflow-y auto
  flex 1
  padding 16px 0 70px

.page-footer
  border-top 2px solid #F5F5F5
  box-sizing border-box

.dividing-line
  height 2px
  background rgba(245,245,245,1)
  margin 32px 0